{
    "Changes": [        
        {
            "Action": "Load",
            "Target": "Maps/{{ModID}}.Maps.Jeremy_Room",
            "FromFile": "assets/Maps/Jeremy_Room.tmx"
        },
        {
            "LogName": "Location Data",
            "Action": "EditData",
            "Target": "Data/Locations",
            "Entries": {
                "{{ModID}}.Maps.Jeremy_Room": {
                    "MusicDefault": "{{ModID}}.Music.JeremyTheme",
                    "DisplayName": "{{i18n:Maps.JeremyTSnail.JeremyRoom}}",
                    "DefaultArrivalTile": { "X": 10, "Y": 6 },
                    "CreateOnLoad": { "MapPath": "Maps/{{ModID}}.Maps.Jeremy_Room" }
                }
            }
        },
        {
            "Action": "EditMap",
            "Target": "Maps/{{ModID}}.Maps.Jeremy_Room",
            "AddWarps": [ 
                "10 4 {{ModID}}.Maps.Trace_House_Interior 11 22",
                "11 4 {{ModID}}.Maps.Trace_House_Interior 12 22" 
            ]
        },

