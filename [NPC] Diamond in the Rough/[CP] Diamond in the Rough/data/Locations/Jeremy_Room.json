            "Action": "EditMap",
            "Target": "Maps/{{ModID}}.Maps.Jeremy_Room",
            "MapTiles": [
                {
                    "Position": { "X": 4, "Y": 6 }, "Layer": "Buildings",
                    "SetProperties": { "Action": "PlayEvent SnailTest true false Dialogue {{i18n:Quests.DitR.VoidSnailRoundUp.QuestActive}}" }
                },
                {
                    "Position": { "X": 5, "Y": 6 }, "Layer": "Buildings",
                    "SetProperties": { "Action": "PlayEvent SnailTest true false Dialogue {{i18n:Quests.DitR.VoidSnailRoundUp.QuestActive}}" }
                }
            ]
        },

