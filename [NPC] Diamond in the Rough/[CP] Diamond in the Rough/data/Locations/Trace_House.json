                "{{ModID}}.TriggerActions.TraceBarbieGirl.FlagRecieved": {
                    "Trigger": "DayStarted",
                    "MarkActionApplied": false,
                    "Id": "{{ModID}}.TriggerActions.TraceBarbieGirl.FlagRecieved",
                    "Condition": "SYNCED_RANDOM day TraceBarbieGirl 0.05 @addDailyLuck",
                    "Actions": [ "AddMail Current DitR_M_TraceBarbieGirl" ]
                },
                "{{ModID}}.TriggerActions.TraceBarbieGirl.FlagRemoved": {
                    "Trigger": "DayEnding",
                    "MarkActionApplied": false,
                    "Id": "{{ModID}}.TriggerActions.TraceBarbieGirl.FlagRemoved",
                    "Condition": "PLAYER_HAS_MAIL Current DitR_M_TraceBarbieGirl Any",
                    "Actions": [  "RemoveMail Current DitR_M_TraceBarbieGirl" ]
                }
        }
